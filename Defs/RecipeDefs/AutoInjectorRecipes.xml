<Defs>

	<!-- Epinephrine Auto-Injector Recipe -->
	<RecipeDef>
		<defName>Make_AutoInjector_Epinephrine</defName>
		<label>make epinephrine auto-injector</label>
		<description>Craft an epinephrine auto-injector for emergency cardiac treatment.</description>
		<jobString>Making epinephrine auto-injector.</jobString>
		<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Drug</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>1200</workAmount>
		<targetCountAdjustment>5</targetCountAdjustment>
		<skillRequirements>
			<Intellectual>6</Intellectual>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MedicineIndustrial</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MedicineIndustrial</li>
				<li>Neutroamine</li>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AutoInjector_Epinephrine>1</AutoInjector_Epinephrine>
		</products>
		<researchPrerequisite>AutoInjectors_Advanced</researchPrerequisite>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
	</RecipeDef>

