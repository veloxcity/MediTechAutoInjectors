<Defs>

	<!-- Base Auto-Injector (Abstract) -->
	<ThingDef Name="AutoInjectorBase" Abstract="True">
		<thingClass>ThingWithComps</thingClass>
		<category>Item</category>
		<drawerType>MapMeshOnly</drawerType>
		<useHitPoints>true</useHitPoints>
		<healthAffectsPrice>true</healthAffectsPrice>
		<selectable>true</selectable>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>0.7</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<Beauty>-4</Beauty>
		</statBases>
		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>true</alwaysHaulable>
		<tickerType>Rare</tickerType>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<pathCost>14</pathCost>
		<resourceReadoutPriority>Last</resourceReadoutPriority>
		<drawGUIOverlay>true</drawGUIOverlay>
		<ingestible>
			<foodType>Processed</foodType>
			<baseIngestTicks>80</baseIngestTicks>
			<chairSearchRadius>0</chairSearchRadius>
			<ingestSound>Ingest_Inject</ingestSound>
			<ingestHoldUsesTable>false</ingestHoldUsesTable>
			<ingestCommandString>Use {0}</ingestCommandString>
			<ingestReportString>Using {0}.</ingestReportString>
		</ingestible>
		<tradeTags>
			<li>Exotic</li>
			<li>Medicine</li>
		</tradeTags>
	</ThingDef>

	<!-- Epinephrine Auto-Injector -->
	<ThingDef ParentName="AutoInjectorBase">
		<defName>AutoInjector_Epinephrine</defName>
		<label>epinephrine auto-injector</label>
		<description>An emergency auto-injector containing epinephrine (adrenaline). Designed for rapid treatment of cardiac arrest, shock, and severe allergic reactions. Dramatically increases consciousness, heart rate, and movement speed.\n\nWarning: Can cause heart strain and increased metabolic demands. Overuse may lead to cardiac complications.</description>
		<graphicData>
			<texPath>Things/Item/Drug/AutoInjector_Epinephrine</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<socialPropernessMatters>false</socialPropernessMatters>
		<rotatable>false</rotatable>
		<stackLimit>10</stackLimit>
		<statBases>
			<WorkToMake>1200</WorkToMake>
			<MarketValue>45</MarketValue>
			<Mass>0.1</Mass>
		</statBases>
		<techLevel>Industrial</techLevel>
		<ingestible>
			<drugCategory>Medical</drugCategory>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>AutoInjectorHigh_Epinephrine</hediffDef>
					<severity>0.75</severity>
				</li>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>AutoInjectorTolerance_Epinephrine</hediffDef>
					<severity>0.045</severity>
					<divideByBodySize>true</divideByBodySize>
				</li>
			</outcomeDoers>
		</ingestible>
		<comps>
			<li Class="CompProperties_Drug">
				<chemical>AutoInjector_Epinephrine</chemical>
				<addictiveness>0.12</addictiveness>
				<minToleranceToAddict>0.15</minToleranceToAddict>
				<existingAddictionSeverityOffset>0.20</existingAddictionSeverityOffset>
				<needLevelOffset>1</needLevelOffset>
				<listOrder>210</listOrder>
			</li>
		</comps>
	</ThingDef>

	<!-- Morphine Auto-Injector -->
